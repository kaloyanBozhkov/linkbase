{
  "version": 2,
  "builds": [
    {
      "src": "backend/dist/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/dist/src/index.js"
    }
  ],
  "installCommand": "pnpm install",
  "functions": {
    "backend/dist/src/index.js": {
      "runtime": "nodejs22.x"
    }
  }
} 