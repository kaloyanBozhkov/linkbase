{
  "extends": [
    "@tsconfig/react-native/tsconfig.json",
    "expo/tsconfig.base"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/common/*": ["../common/*"],
      "@/linkbase/*": ["src/*"]
    },
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "lib": [
      "ES2015",
      "ES2017",
      "ES2018",
      "ES2020"
    ],
    "target": "ES2018",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "src/**/*",
    "App.tsx",
    "index.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "../common/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
}
