{
  "name": "linkbase-app",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:unused": "eslint . --fix --rule 'unused-imports/no-unused-imports: error'",
    "clean-imports:simple": "eslint . --fix --rule 'unused-imports/no-unused-imports: error'",
