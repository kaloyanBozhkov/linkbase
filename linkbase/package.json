    "test": "jest",
    "type-check": "tsc --noEmit",
    "update-ip": "node ../packages/shared/src/scripts/update-dev-ip.mjs",
    "dev": "pnpm run update-ip && npx expo start",
    "translate": "node ../packages/shared/src/scripts/translate-with-gpt.js",
    "translate:force": "node ../packages/shared/src/scripts/translate-with-gpt.js --force",
